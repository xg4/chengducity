{
  "name": "chengducity",
  "version": "0.1.0",
  "description": "telegram bot",
  "keywords": [
    "chengducity",
    "houses",
    "telegram",
    "bot"
  ],
  "homepage": "https://github.com/xg4/chengducity#readme",
  "bugs": {
    "url": "https://github.com/xg4/chengducity/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xg4/chengducity.git"
  },
  "license": "MIT",
  "author": "xg4 <xingor4@gmail.com>",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "db:generate": "npm run typeorm migration:generate -- -p -n",
    "db:drop": "npm run typeorm schema:drop",
    "db:migrate": "npm run typeorm migration:run",
    "serve": "ts-node -r tsconfig-paths/register server/app.ts",
    "dev": "nodemon --watch server --ext ts --exec npm run serve",
    "start": "cross-env NODE_ENV=production npm run serve",
    "format": "prettier --write client/**/*.ts* server/**/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.2",
    "@types/node-fetch": "^2.5.10",
    "@types/uuid": "^8.3.0",
    "apollo-server-express": "^2.24.0",
    "cheerio": "^1.0.0-rc.6",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.4",
    "debug": "^4.3.1",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "telegraf": "^4.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.32",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
